  <insert id="insertRoomInfo" parameterType="petweens.model.RoomInfo">
  	INSERT INTO roominfo(roomname,roomowner,ispasswd,password,path,filename,endpage) values(#{roomname},#{roomowner},#{ispasswd},#{password},#{path},#{filename},#{endpage})
  </insert>
  
   <select id="getRoomInfo" resultType="petweens.model.RoomInfo">
   SELECT id,roomname,roomowner,ispasswd,password,filename,endpage FROM roominfo
  </select>
</mapper>